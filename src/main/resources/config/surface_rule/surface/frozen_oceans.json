            "offset": 0,
            "surface_depth_multiplier": 0
          },
          "then_run": {
            "type": "minecraft:block",
            "result_state": {
              "Name": "minecraft:air"
            }
          }
        },
        {
          "type": "minecraft:condition",
          "if_true": {
            "type": "minecraft:temperature"
          },
          "then_run": {
            "type": "minecraft:block",
